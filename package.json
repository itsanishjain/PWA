{
    "$schema": "https://json.schemastore.org/package",
    "name": "pool-pwa",
    "version": "0.0.0",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "build": "next build",
        "check-types": "tsc --noemit",
        "dev": "next dev",
        "dev:turbo": "TURBO=true next dev --turbo",
        "gen": "node scripts/generate-types.mjs",
        "gen:contracts": "pnpm gen contracts",
        "gen:db": "pnpm gen db",
        "lint": "next lint",
        "preinstall": "npx only-allow pnpm",
        "prettify": "prettier -w",
        "start": "next start",
        "test": "vitest"
    },
    "dependencies": {
        "@coinbase/cbpay-js": "2.2.0",
        "@gatefi/js-sdk": "0.18.0",
        "@privy-io/react-auth": "1.76.4",
        "@privy-io/server-auth": "1.9.5",
        "@privy-io/wagmi": "0.2.12",
        "@radix-ui/react-alert-dialog": "1.1.1",
        "@radix-ui/react-avatar": "1.1.0",
        "@radix-ui/react-dialog": "1.1.1",
        "@radix-ui/react-dropdown-menu": "2.1.1",
        "@radix-ui/react-label": "2.1.0",
        "@radix-ui/react-popover": "1.1.1",
        "@radix-ui/react-progress": "1.1.0",
        "@radix-ui/react-select": "2.1.1",
        "@radix-ui/react-slot": "1.1.0",
        "@radix-ui/react-tabs": "1.1.0",
        "@serwist/next": "9.0.5",
        "@stripe/crypto": "0.0.4",
        "@supabase/ssr": "0.4.0",
        "@supabase/supabase-js": "2.45.0",
        "@tanstack/query-sync-storage-persister": "5.51.15",
        "@tanstack/react-query": "5.51.15",
        "@tanstack/react-query-persist-client": "5.51.15",
        "@wagmi/core": "2.13.1",
        "class-variance-authority": "0.7.0",
        "clsx": "2.1.1",
        "cmdk": "1.0.0",
        "date-fns": "3.6.0",
        "date-fns-tz": "3.1.3",
        "framer-motion": "11.3.19",
        "image-type": "5.2.0",
        "js-cookie": "3.0.5",
        "jsonwebtoken": "9.0.2",
        "lodash": "4.17.21",
        "lucide-react": "0.417.0",
        "native-querystring": "1.1.1",
        "next": "14.2.5",
        "permissionless": "0.1.43",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-icons": "5.2.1",
        "react-qr-code": "2.0.15",
        "react-qr-reader": "3.0.0-beta-1",
        "serwist": "9.0.5",
        "sharp": "0.33.4",
        "sonner": "1.5.0",
        "tailwind-merge": "2.4.0",
        "tailwindcss-animate": "1.0.7",
        "vaul": "0.9.1",
        "viem": "2.18.5",
        "wagmi": "2.12.1",
        "zsa": "0.5.1"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "14.2.5",
        "@playwright/test": "1.46.1",
        "@tanstack/eslint-plugin-query": "5.51.15",
        "@tanstack/react-query-devtools": "5.51.15",
        "@testing-library/jest-dom": "6.4.8",
        "@testing-library/react": "16.0.0",
        "@testing-library/react-hooks": "8.0.1",
        "@testing-library/user-event": "14.5.2",
        "@types/js-cookie": "3.0.6",
        "@types/jsonwebtoken": "9.0.6",
        "@types/lodash": "4.17.7",
        "@types/node": "22.0.0",
        "@types/react": "18.3.3",
        "@types/react-dom": "18.3.0",
        "@typescript-eslint/eslint-plugin": "7.18.0",
        "@vitejs/plugin-react": "4.3.1",
        "@wagmi/cli": "2.1.15",
        "autoprefixer": "10.4.19",
        "cssnano": "7.0.4",
        "eslint": "8",
        "eslint-config-next": "14.2.5",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-tailwindcss": "3.17.4",
        "ignore-loader": "0.1.2",
        "jsdom": "24.1.1",
        "postcss": "8.4.40",
        "prettier": "3.3.3",
        "prettier-plugin-tailwindcss": "0.6.5",
        "tailwindcss": "3.4.7",
        "tailwindcss-safe-area": "0.5.1",
        "typescript": "5.5.4",
        "vite-tsconfig-paths": "4.3.2",
        "vitest": "2.0.4"
    },
    "pnpm": {
        "overrides": {
            "@types/react": "18.3.3",
            "react": "18.3.1",
            "react-dom": "18.3.1",
            "rimraf": "6.0.1"
        },
        "allowedDeprecatedVersions": {
            "@babel/plugin-proposal-async-generator-functions": "7.20.7",
            "@babel/plugin-proposal-class-properties": "7.18.6",
            "@babel/plugin-proposal-logical-assignment-operators": "7.20.7",
            "@babel/plugin-proposal-nullish-coalescing-operator": "7.18.6",
            "@babel/plugin-proposal-numeric-separator": "7.18.6",
            "@babel/plugin-proposal-object-rest-spread": "7.20.7",
            "@babel/plugin-proposal-optional-catch-binding": "7.18.6",
            "@babel/plugin-proposal-optional-chaining": "7.21.0",
            "@humanwhocodes/config-array": "0.11.14",
            "@humanwhocodes/object-schema": "2.0.3",
            "@motionone/vue": "10.16.4",
            "glob": "7.2.3",
            "inflight": "1.0.6",
            "querystring": "0.2.1"
        }
    },
    "prettier": {
        "arrowParens": "avoid",
        "bracketSameLine": true,
        "bracketSpacing": true,
        "endOfLine": "lf",
        "jsxSingleQuote": true,
        "printWidth": 120,
        "proseWrap": "preserve",
        "quoteProps": "consistent",
        "semi": false,
        "singleQuote": true,
        "trailingComma": "all",
        "useTabs": false,
        "tabWidth": 4,
        "plugins": [
            "prettier-plugin-tailwindcss"
        ],
        "tailwindConfig": "./tailwind.config.ts",
        "tailwindFunctions": [
            "clsx",
            "classnames",
            "cn",
            "cva",
            "tw",
            "twMerge"
        ]
    },
    "eslintConfig": {
        "parserOptions": {
            "project": "./tsconfig.eslint.json"
        },
        "extends": [
            "next/core-web-vitals",
            "plugin:@typescript-eslint/recommended-type-checked",
            "plugin:@tanstack/eslint-plugin-query/recommended",
            "plugin:tailwindcss/recommended",
            "prettier"
        ],
        "rules": {
            "react/self-closing-comp": 1,
            "@typescript-eslint/no-unused-vars": [
                1,
                {
                    "argsIgnorePattern": "^_",
                    "varsIgnorePattern": "^_"
                }
            ],
            "@typescript-eslint/consistent-type-imports": 1
        }
    }
}
